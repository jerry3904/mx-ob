<?xml version='1.0' encoding='utf-8'?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                                   file:///usr/share/openbox/menu.xsd">
  <menu id="root-menu" label="Openbox 3">
    <item label="All apps">
      <action name="Execute">
        <execute>jgmenu_run</execute>
      </action>
    </item>
    <separator />
    <item label="Browser">
      <action name="Execute">
        <execute>x-www-browser</execute>
      </action>
    </item>
    <item label="File manager">
      <action name="Execute">
        <execute>thunar</execute>
      </action>
    </item>
    <menu id="menu-4258592" label="Help">
      <item label="First Aid">
        <action name="Execute">
          <execute>mxob-help</execute>
        </action>
      </item>
      <item label="MX Users Manual">
        <action name="Execute">
          <execute>mx-manual</execute>
        </action>
      </item>
    </menu>
    <item label="MX Tools">
      <action name="Execute">
        <execute>mx-tools</execute>
      </action>
    </item>
    <item label="Run">
      <action name="Execute">
        <execute>/usr/bin/mxfb-rofirun</execute>
      </action>
    </item>
    <item label="Terminal">
      <action name="Execute">
        <execute>xfce4-terminal --drop-down</execute>
      </action>
    </item>
    <separator />
    <menu id="Appearance.menu" label="Appearance">
      <item label="Conky">
        <action name="Execute">
          <execute>conky-manager2</execute>
        </action>
      </item>
      <item label="Desktop icons">
        <action name="Execute">
          <execute>mx-idesktool</execute>
        </action>
      </item>
      <item label="Dock">
        <action name="Execute">
          <execute>tint2session</execute>
        </action>
      </item>
      <item label="Monitors">
        <action name="Execute">
          <execute>gkrellm</execute>
        </action>
      </item>
      <item label="Theme">
        <action name="Execute">
          <execute>obconf-qt</execute>
        </action>
      </item>
      <item label="Wallpaper">
        <action name="Execute">
          <execute>/usr/bin/feh-setbg</execute>
        </action>
      </item>
    </menu>
    <menu id="Settings.menu" label="Settings">
      <item label="Autostart">
        <action name="Execute">
          <execute>featherpad ~/.config/openbox/autostart</execute>
        </action>
      </item>
      <item label="Detailed ">
        <action name="Execute">
          <execute>featherpad ~/.config/openbox/rc.xml</execute>
        </action>
      </item>
      <item label="Dock">
        <action name="Execute">
          <execute>mxfb-tint2-manager</execute>
        </action>
      </item>
      <item label="Display">
        <action name="Execute">
          <execute>raindrop</execute>
        </action>
      </item>
      <item label="Keys">
        <action name="Execute">
          <execute>obkey</execute>
        </action>
      </item>
      <menu id="menu-7065141" label="Menu">
        <item label="All apps">
          <action name="Execute">
            <execute>featherpad ~/.config/jgmenu/jgmenurc</execute>
          </action>
        </item>
        <item label="Desktop">
          <action name="Execute">
            <execute>obmenu2</execute>
          </action>
        </item>
        <item label="Rofi">
          <action name="Execute">
            <execute>mx-rofi-manager</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2893040" label="Notification area">
        <item label="Systray">
          <action name="Execute">
            <execute>exo-open ~/.stalonetrayrc</execute>
          </action>
        </item>
        <item label="Clock">
          <action name="Execute">
            <execute>exo-open ~/.Xdefaults</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2782497" label="Raspberry Pi">
        <item label="Basic setup">
          <action name="Execute">
            <execute>rc_gui</execute>
          </action>
        </item>
        <item label="Detailed config">
          <action name="Execute">
            <execute>x-terminal-emulator -e sudo raspi-config</execute>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="Out_of_sight.menu" label="Out of sight">
      <item label="Conky">
        <action name="Execute">
          <execute>conkytoggle.sh</execute>
        </action>
      </item>
      <item label="Desktop icons">
        <action name="Execute">
          <execute>idesktoggle icons</execute>
        </action>
      </item>
      <item label="Dock">
        <action name="Execute">
          <execute>toggletint2_dock_ob</execute>
        </action>
      </item>
      <item label="Monitors ">
        <action name="Execute">
          <execute>toggle-mx gkrellm</execute>
        </action>
      </item>
      <menu id="menu-6251006" label="Notification area">
        <item label="Systray">
          <action name="Execute">
            <execute>toggle-mx stalonetray</execute>
          </action>
        </item>
        <item label="Clock">
          <action name="Execute">
            <execute>toggle-mx tdc</execute>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="exit-menu" label="Leave">
      <item label="Refresh">
        <action name="Execute">
          <execute>openbox --reconfigure</execute>
        </action>
      </item>
      <item label="Log Out">
        <action name="Execute">
          <command>openbox --exit</command>
        </action>
      </item>
      <item label="Shutdown">
        <action name="Execute">
          <command>systemctl poweroff</command>
        </action>
      </item>
      <item label="Restart">
        <action name="Execute">
          <command>systemctl reboot</command>
        </action>
      </item>
    </menu>
  </menu>
</openbox_menu>
