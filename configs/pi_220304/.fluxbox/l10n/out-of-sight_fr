[begin] (Masquer) 
    [exec] (Arrêter le dock) { killall wmalauncher }
    [exec] (Arrêter une fenêtre) { xkill }
    [separator]
    [exec] (Basculer conky) { conkytoggle.sh }
    [exec] (Basculer l'iDesk) { idesktoggle idesk }
    [exec] (Basculer les icônes) { idesktoggle icons }
    [exec] (Basculer masquage auto. dock) { $HOME/.fluxbox/scripts/toggledock }
    [exec] (Basculer masquage auto. barre d'outils) {$HOME/.fluxbox/scripts/toggletoolbar}
    [exec] (Basculer masquage auto. du panneau) {$HOME/.fluxbox/scripts/toggletint2}
    [exec] (Toggle autohide panel-dock) {$HOME/.fluxbox/scripts/toggletint2-dock}
    [exec] (Basculer les moniteurs) { toggle-mx gkrellm }
    [separator]
    [exec] (Désactiver le dock par défaut) { killall wmalauncher && $HOME/.fluxbox/scripts/dfltdck_kill }
[end]
