<?xml version='1.0' encoding='utf-8'?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                                   file:///usr/share/openbox/menu.xsd">
  <menu id="root-menu" label="Openbox 3">
    <item label="所有应用程序">
      <action name="Execute">
        <execute>jgmenu_run</execute>
      </action>
    </item>
    <separator />
    <item label="浏览器">
      <action name="Execute">
        <execute>x-www-browser</execute>
      </action>
    </item>
    <item label="文件管理器">
      <action name="Execute">
        <execute>thunar</execute>
      </action>
    </item>
    <menu id="menu-4258592" label="Help">
      <item label="急救">
        <action name="Execute">
          <execute>mxob-help</execute>
        </action>
      </item>
      <item label="MX 用户手册">
        <action name="Execute">
          <execute>mx-manual</execute>
        </action>
      </item>
    </menu>
    <item label="MX 工具">
      <action name="Execute">
        <execute>mx-tools</execute>
      </action>
    </item>
    <item label="运行">
      <action name="Execute">
        <execute>/usr/bin/mxfb-rofirun</execute>
      </action>
    </item>
    <item label="终端">
      <action name="Execute">
        <execute>xfce4-terminal --drop-down</execute>
      </action>
    </item>
    <separator />
    <menu id="Appearance.menu" label="外观">
      <item label="康基">
        <action name="Execute">
          <execute>conky-manager2</execute>
        </action>
      </item>
      <item label="桌面图标">
        <action name="Execute">
          <execute>mx-idesktool</execute>
        </action>
      </item>
      <item label="桌面">
        <action name="Execute">
          <execute>tint2 -c ~/.config/tint2/dock-basic-tint2rc</execute>
        </action>
      </item>
      <item label="监控器">
        <action name="Execute">
          <execute>gkrellm</execute>
        </action>
      </item>
      <item label="主题">
        <action name="Execute">
          <execute>obconf-qt</execute>
        </action>
      </item>
      <item label="壁纸">
        <action name="Execute">
          <execute>/usr/bin/feh-setbg</execute>
        </action>
      </item>
    </menu>
    <menu id="Settings.menu" label="设置">
      <item label="自动启动">
        <action name="Execute">
          <execute>featherpad ~/.config/openbox/autostart</execute>
        </action>
      </item>
      <item label="详细配置">
        <action name="Execute">
          <execute>featherpad ~/.config/openbox/rc.xml</execute>
        </action>
      </item>
      <item label="桌面">
        <action name="Execute">
          <execute>tint2conf</execute>
        </action>
      </item>
      <item label="显示">
        <action name="Execute">
          <execute>raindrop</execute>
        </action>
      </item>
      <item label="按键">
        <action name="Execute">
          <execute>obkey</execute>
        </action>
      </item>
      <menu id="menu-7065141" label="菜单">
        <item label="所有应用程序">
          <action name="Execute">
            <execute>featherpad ~/.config/jgmenu/jgmenurc</execute>
          </action>
        </item>
        <item label="桌面">
          <action name="Execute">
            <execute>obmenu2</execute>
          </action>
        </item>
        <item label="重启">
          <action name="Execute">
            <execute>mx-rofi-manager</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2893040" label="通知区域">
        <item label="系统盘">
          <action name="Execute">
            <execute>exo-open ~/.stalonetrayrc</execute>
          </action>
        </item>
        <item label="时钟">
          <action name="Execute">
            <execute>exo-open ~/.Xdefaults</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2782497" label="树莓派">
        <item label="基本设置">
          <action name="Execute">
            <execute>rc_gui</execute>
          </action>
        </item>
        <item label="详细配置">
          <action name="Execute">
            <execute>x-terminal-emulator -e sudo raspi-config</execute>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="Out_of_sight.menu" label="看不见的地方">
      <item label="康基">
        <action name="Execute">
          <execute>conkytoggle.sh</execute>
        </action>
      </item>
      <item label="桌面图标">
        <action name="Execute">
          <execute>idesktoggle icons</execute>
        </action>
      </item>
      <item label="桌面">
        <action name="Execute">
          <execute>toggletint2_dock_ob</execute>
        </action>
      </item>
      <item label="Monitors ">
        <action name="Execute">
          <execute>toggle-mx gkrellm</execute>
        </action>
      </item>
      <menu id="menu-6251006" label="通知区域">
        <item label="系统盘">
          <action name="Execute">
            <execute>toggle-mx stalonetray</execute>
          </action>
        </item>
        <item label="时钟">
          <action name="Execute">
            <execute>toggle-mx tdc</execute>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="exit-menu" label="离开">
      <item label="刷新">
        <action name="Execute">
          <execute>openbox --reconfigure</execute>
        </action>
      </item>
      <item label="注销">
        <action name="Execute">
          <command>openbox --exit</command>
        </action>
      </item>
      <item label="关机">
        <action name="Execute">
          <command>systemctl poweroff</command>
        </action>
      </item>
      <item label="重新启动">
        <action name="Execute">
          <command>systemctl reboot</command>
        </action>
      </item>
    </menu>
  </menu>
</openbox_menu>
