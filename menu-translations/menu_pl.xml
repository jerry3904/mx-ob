<?xml version='1.0' encoding='utf-8'?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                                   file:///usr/share/openbox/menu.xsd">
  <menu id="root-menu" label="Openbox 3">
    <item label="Wszystkie aplikacje">
      <action name="Execute">
        <execute>jgmenu_run</execute>
      </action>
    </item>
    <separator />
    <item label="Przeglądarka">
      <action name="Execute">
        <execute>x-www-browser</execute>
      </action>
    </item>
    <item label="Menedżer plików">
      <action name="Execute">
        <execute>thunar</execute>
      </action>
    </item>
    <menu id="menu-4258592" label="Help">
      <item label="Pierwsza pomoc">
        <action name="Execute">
          <execute>mxob-help</execute>
        </action>
      </item>
      <item label="Podręcznik użytkownika MX">
        <action name="Execute">
          <execute>mx-manual</execute>
        </action>
      </item>
    </menu>
    <item label="Narzędzia MX">
      <action name="Execute">
        <execute>mx-tools</execute>
      </action>
    </item>
    <item label="Uruchom">
      <action name="Execute">
        <execute>/usr/bin/mxfb-rofirun</execute>
      </action>
    </item>
    <item label="Terminal">
      <action name="Execute">
        <execute>xfce4-terminal --drop-down</execute>
      </action>
    </item>
    <separator />
    <menu id="Appearance.menu" label="Wygląd">
      <item label="Conky">
        <action name="Execute">
          <execute>conky-manager2</execute>
        </action>
      </item>
      <item label="Ikony pulpitu">
        <action name="Execute">
          <execute>mx-idesktool</execute>
        </action>
      </item>
      <item label="Dok">
        <action name="Execute">
          <execute>tint2session</execute>
        </action>
      </item>
      <item label="Monitory">
        <action name="Execute">
          <execute>gkrellm</execute>
        </action>
      </item>
      <item label="Motyw">
        <action name="Execute">
          <execute>obconf-qt</execute>
        </action>
      </item>
      <item label="Tapeta">
        <action name="Execute">
          <execute>/usr/bin/feh-setbg</execute>
        </action>
      </item>
    </menu>
    <menu id="Settings.menu" label="Ustawienia">
      <item label="Autostart">
        <action name="Execute">
          <execute>xdg-open ~/.config/openbox/autostart</execute>
        </action>
      </item>
      <item label="Szczegółowe ">
        <action name="Execute">
          <execute>xdg-open ~/.config/openbox/rc.xml</execute>
        </action>
      </item>
      <item label="Dok">
        <action name="Execute">
          <execute>mxfb-tint2-manager</execute>
        </action>
      </item>
      <item label="Wyświetlacz">
        <action name="Execute">
          <execute>raindrop</execute>
        </action>
      </item>
      <item label="Klawisze">
        <action name="Execute">
          <execute>obkey</execute>
        </action>
      </item>
      <menu id="menu-7065141" label="Menu">
        <item label="Wszystkie aplikacje">
          <action name="Execute">
            <execute>xdg-open ~/.config/jgmenu/jgmenurc</execute>
          </action>
        </item>
        <item label="Pulpit">
          <action name="Execute">
            <execute>obmenu2</execute>
          </action>
        </item>
        <item label="Rofi">
          <action name="Execute">
            <execute>mx-rofi-manager</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2893040" label="Obszar powiadomień">
        <item label="System">
          <action name="Execute">
            <execute>xdg-open ~/.stalonetrayrc</execute>
          </action>
        </item>
        <item label="Zegar">
          <action name="Execute">
            <execute>xdg-open ~/.Xdefaults</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2782497" label="Raspberry Pi">
        <item label="Konfiguracja podstawowa">
          <action name="Execute">
            <execute>rc_gui</execute>
          </action>
        </item>
        <item label="Szczegółowa konfiguracja">
          <action name="Execute">
            <execute>x-terminal-emulator -e sudo raspi-config</execute>
          </action>
        </item>
      </menu>
            <item label="Tapeta">
        <action name="Execute">
          <execute>xdg-open ~/.config/feh/feh-setbg.conf</execute>
        </action>
      </item>
    </menu>
    <menu id="Out_of_sight.menu" label="Poza zasięgiem wzroku">
      <item label="Conky">
        <action name="Execute">
          <execute>conkytoggle.sh</execute>
        </action>
      </item>
      <item label="Ikony pulpitu">
        <action name="Execute">
          <execute>idesktoggle icons</execute>
        </action>
      </item>
      <item label="Dok">
        <action name="Execute">
          <execute>toggletint2_dock_ob</execute>
        </action>
      </item>
      <item label="Monitors ">
        <action name="Execute">
          <execute>toggle-mx gkrellm</execute>
        </action>
      </item>
      <menu id="menu-6251006" label="Obszar powiadomień">
        <item label="System">
          <action name="Execute">
            <execute>toggle-mx stalonetray</execute>
          </action>
        </item>
        <item label="Zegar">
          <action name="Execute">
            <execute>toggle-mx tdc</execute>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="exit-menu" label="Opuść">
      <item label="Odświeżanie">
        <action name="Execute">
          <execute>openbox --reconfigure</execute>
        </action>
      </item>
      <item label="Wyloguj">
        <action name="Execute">
          <command>openbox --exit</command>
        </action>
      </item>
      <item label="Wyłączanie">
        <action name="Execute">
          <command>systemctl poweroff</command>
        </action>
      </item>
      <item label="Restart">
        <action name="Execute">
          <command>systemctl reboot</command>
        </action>
      </item>
    </menu>
  </menu>
</openbox_menu>
