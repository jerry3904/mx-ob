<?xml version='1.0' encoding='utf-8'?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                                   file:///usr/share/openbox/menu.xsd">
  <menu id="root-menu" label="Openbox 3">
    <item label="Todas as aplicações">
      <action name="Execute">
        <execute>jgmenu_run</execute>
      </action>
    </item>
    <separator />
    <item label="Navegador">
      <action name="Execute">
        <execute>x-www-browser</execute>
      </action>
    </item>
    <item label="Gestor de ficheiros">
      <action name="Execute">
        <execute>thunar</execute>
      </action>
    </item>
    <menu id="menu-4258592" label="Help">
      <item label="Primeiros socorros">
        <action name="Execute">
          <execute>mxob-help</execute>
        </action>
      </item>
      <item label="Manual do utilizador MX">
        <action name="Execute">
          <execute>mx-manual</execute>
        </action>
      </item>
    </menu>
    <item label="Ferramentas MX">
      <action name="Execute">
        <execute>mx-tools</execute>
      </action>
    </item>
    <item label="Executar">
      <action name="Execute">
        <execute>/usr/bin/mxfb-rofirun</execute>
      </action>
    </item>
    <item label="Terminal">
      <action name="Execute">
        <execute>xfce4-terminal --drop-down</execute>
      </action>
    </item>
    <separator />
    <menu id="Appearance.menu" label="Aparência">
      <item label="Conky">
        <action name="Execute">
          <execute>conky-manager2</execute>
        </action>
      </item>
      <item label="Ícones do ambiente de trabalho">
        <action name="Execute">
          <execute>mx-idesktool</execute>
        </action>
      </item>
      <item label="Dock">
        <action name="Execute">
          <execute>tint2 -c ~/.config/tint2/dock-basic-tint2rc</execute>
        </action>
      </item>
      <item label="Monitores">
        <action name="Execute">
          <execute>gkrellm</execute>
        </action>
      </item>
      <item label="Tema">
        <action name="Execute">
          <execute>obconf-qt</execute>
        </action>
      </item>
      <item label="Papel de parede">
        <action name="Execute">
          <execute>/usr/bin/feh-setbg</execute>
        </action>
      </item>
    </menu>
    <menu id="Settings.menu" label="Configurações">
      <item label="Arranque automático">
        <action name="Execute">
          <execute>featherpad ~/.config/openbox/autostart</execute>
        </action>
      </item>
      <item label="Detalhado ">
        <action name="Execute">
          <execute>featherpad ~/.config/openbox/rc.xml</execute>
        </action>
      </item>
      <item label="Dock">
        <action name="Execute">
          <execute>tint2conf</execute>
        </action>
      </item>
      <item label="Ecrã">
        <action name="Execute">
          <execute>raindrop</execute>
        </action>
      </item>
      <item label="Teclas">
        <action name="Execute">
          <execute>obkey</execute>
        </action>
      </item>
      <menu id="menu-7065141" label="Menu">
        <item label="Todas as aplicações">
          <action name="Execute">
            <execute>featherpad ~/.config/jgmenu/jgmenurc</execute>
          </action>
        </item>
        <item label="Ambiente de trabalho">
          <action name="Execute">
            <execute>obmenu2</execute>
          </action>
        </item>
        <item label="Rofi">
          <action name="Execute">
            <execute>mx-rofi-manager</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2893040" label="Área de notificação">
        <item label="Systray">
          <action name="Execute">
            <execute>exo-open ~/.stalonetrayrc</execute>
          </action>
        </item>
        <item label="Relógio">
          <action name="Execute">
            <execute>exo-open ~/.Xdefaults</execute>
          </action>
        </item>
      </menu>
      <menu id="menu-2782497" label="Raspberry Pi">
        <item label="Configuração básica">
          <action name="Execute">
            <execute>rc_gui</execute>
          </action>
        </item>
        <item label="Configuração detalhada">
          <action name="Execute">
            <execute>x-terminal-emulator -e sudo raspi-config</execute>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="Out_of_sight.menu" label="Fora de vista">
      <item label="Conky">
        <action name="Execute">
          <execute>conkytoggle.sh</execute>
        </action>
      </item>
      <item label="Ícones do ambiente de trabalho">
        <action name="Execute">
          <execute>idesktoggle icons</execute>
        </action>
      </item>
      <item label="Dock">
        <action name="Execute">
          <execute>toggletint2_dock_ob</execute>
        </action>
      </item>
      <item label="Monitors ">
        <action name="Execute">
          <execute>toggle-mx gkrellm</execute>
        </action>
      </item>
      <menu id="menu-6251006" label="Área de notificação">
        <item label="Systray">
          <action name="Execute">
            <execute>toggle-mx stalonetray</execute>
          </action>
        </item>
        <item label="Relógio">
          <action name="Execute">
            <execute>toggle-mx tdc</execute>
          </action>
        </item>
      </menu>
    </menu>
    <menu id="exit-menu" label="Sair">
      <item label="Atualizar">
        <action name="Execute">
          <execute>openbox --reconfigure</execute>
        </action>
      </item>
      <item label="Terminar sessão">
        <action name="Execute">
          <command>openbox --exit</command>
        </action>
      </item>
      <item label="Desligar">
        <action name="Execute">
          <command>systemctl poweroff</command>
        </action>
      </item>
      <item label="Reiniciar">
        <action name="Execute">
          <command>systemctl reboot</command>
        </action>
      </item>
    </menu>
  </menu>
</openbox_menu>
